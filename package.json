    "url": "git+https://github.com/telemark/tfk-search-index.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/tfk-search-index/issues"
  },
  "homepage": "https://github.com/telemark/tfk-search-index#readme",
  "devDependencies": {
    "coveralls": "3.0.3",
    "standard": "12.0.1",
    "tap": "13.1.2"
  },
  "dependencies": {
    "axios": "0.18.0",
    "cheerio": "1.0.0-rc.3",
    "dotenv": "7.0.0",
    "jsonwebtoken": "8.5.1",
    "normalize-html-whitespace": "1.0.0",
